<?php
namespace MovieWall\ApiBundle\Document;

use Symfony\Component\Validator\Constraints as Assert;
use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Watchlist
{
    /**
     * @MongoDB\Id
     */
    protected $id;

    /**
     * @MongoDB\Timestamp
     */
    protected $addedAt;
}